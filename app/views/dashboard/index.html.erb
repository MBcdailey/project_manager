<!-- RECENT PROJECTS -->
<div class="row header-row px-3 py-3">
  <div class="col-12">
    <h2>Recent Projects</h2>
  </div>
</div>
<% @projects.each do |project| %>
<div class="row project-row px-3 py-3">
  <div class="col-12 py-2 border-bottom">
    <h3><a href="<%= project_path(project.id)%>"><%= project.title %></a></h3>
    <p><%= project.description %></p>
    <p>Created: <%= format_date_time(project.created_at) %></p>
  </div>
</div>
<% end %>
